{
  "name": "bootnode-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "analyze": "ANALYZE=true yarn build",
    "build": "next build",
    "export": "next export",
    "dev": "next dev",
    "lint": "next lint",
    "lint:fix": "eslint --fix \"./**/*.{js,jsx,ts,tsx}\"",
    "prepare": "husky install",
    "prettier:fix": "prettier --write \"./**/*.{js,jsx,ts,tsx}\"",
    "start": "next start",
    "typechain": "typechain --target=ethers-v5 ./src/contracts/abis/**/*.json --out-dir ./types/generated/typechain/",
    "subgraph-codegen": "graphql-codegen --config subgraph-config.js",
    "types-gen": "yarn typechain && yarn subgraph-codegen",
    "postinstall": "yarn typechain && yarn subgraph-codegen",
    "test": "jest"
  },
  "dependencies": {
    "@uniswap/sdk-core": "^3.2.2",
    "@uniswap/token-lists": "^1.0.0-beta.31",
    "@web3-onboard/common": "^2.3.3",
    "@web3-onboard/core": "^2.20.4",
    "@web3-onboard/frame": "^2.0.1",
    "@web3-onboard/gnosis": "^2.1.10",
    "@web3-onboard/injected-wallets": "^2.4.0",
    "@web3-onboard/react": "^2.8.9",
    "@web3-onboard/walletconnect": "^2.4.2",
    "big-number-input": "^1.0.3",
    "ethers": "^5.7.2",
    "graphql": "^16.6.0",
    "graphql-request": "^5.1.0",
    "graphql-tag": "^2.12.6",
    "lodash": "^4.17.21",
    "memoizee": "^0.4.15",
    "next": "^13.1.1",
    "nextjs-google-analytics": "^2.2.2",
    "nullthrows": "^1.1.1",
    "polished": "^4.2.2",
    "react": "^18.2.0",
    "react-debounce-input": "^3.3.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^3.1.4",
    "react-hot-toast": "^2.4.0",
    "react-tooltip": "^5.8.3",
    "recharts": "^2.4.3",
    "sanitize.css": "^13.0.0",
    "styled-components": "^5.3.6",
    "swr": "^2.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@graphql-codegen/cli": "2.16.2",
    "@graphql-codegen/introspection": "2.2.3",
    "@graphql-codegen/typescript": "2.8.6",
    "@graphql-codegen/typescript-graphql-request": "4.5.8",
    "@graphql-codegen/typescript-operations": "2.5.11",
    "@graphql-codegen/typescript-react-apollo": "3.3.7",
    "@next/bundle-analyzer": "^13.1.1",
    "@typechain/ethers-v5": "^10.2.0",
    "@types/jest": "^29.2.5",
    "@types/lodash": "^4.14.191",
    "@types/memoizee": "^0.4.8",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.47.1",
    "@typescript-eslint/parser": "^5.47.1",
    "eslint": "^8.30.0",
    "eslint-config-next": "^13.1.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-sort-destructure-keys": "^1.4.0",
    "graphql-codegen-plugin-typescript-swr": "^0.8.3",
    "husky": "^8.0.2",
    "jest": "^29.3.1",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.1",
    "typechain": "^8.1.1",
    "typescript": "^4.9.4",
    "typescript-graphql-request": "^4.4.6"
  },
  "engines": {
    "npm": ">=8.0.0",
    "node": ">=16.4.2"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --cache --fix",
      "prettier --write",
      "bash -c tsc"
    ]
  },
  "resolutions": {
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.10",
    "ethers": "^5.7.2"
  }
}
